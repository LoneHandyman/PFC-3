{
    "summer":{"heads":8, 
             "d_model": 256, 
             "hidden":512,
             "name":"summer-(lm)",
             "lr":1e-4,
             "d_conv":5,
             "feature-extractors": ["summer:8"]},

    "fnet":{"heads":1, 
            "d_model": 192, 
            "hidden":512,
            "name":"fnet-(lm)",
            "lr":1e-4,
            "feature-extractors": ["fnet:8"]},

    "transformer":{"heads":8, 
                   "d_model": 192, 
                   "hidden":512,
                   "name":"tfmr-(lm)",
                   "lr":1e-5,
                   "feature-extractors": ["attn:8"]},

    "hybrid-fnet-attn":{"heads":8, 
                        "d_model": 192, 
                        "hidden":512,
                        "name":"h-afnet-(lm)",
                        "lr":1e-4,
                        "feature-extractors": ["attn:2", "fnet:6"]},

    "hybrid-f2net-attn":{"heads":8, 
                         "d_model": 192, 
                         "hidden":512,
                         "name":"h-af2net-(lm)",
                         "lr":1e-5,
                         "d_conv":5,
                         "feature-extractors": ["attn:2", "fnet:6"]}

}